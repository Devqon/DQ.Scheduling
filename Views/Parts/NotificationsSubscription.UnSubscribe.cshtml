@using Orchard.Utility.Extensions

@{
    var id = Model.ContentPart.Id;
}

@using (Html.BeginFormAntiForgeryPost(Url.Action("UnSubscribe", new {controller = "Notifications", area = "DQ.Scheduling", id = id, returnUrl = Context.Request.ToUrlString()}), FormMethod.Post)) {
    <button class="primaryButton" type="submit">@T("UnSubscribe")</button>
}