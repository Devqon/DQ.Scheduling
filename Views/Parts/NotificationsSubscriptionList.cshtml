@{
    Script.Require("NotificationsSubscribe_List");
}

<fieldset>
    
    @if (Model.Count > 0) {
        <a href="#" class="subscriptions-header">@T("Subscriptions")</a>

        <div class="subscriptions-list">
            @Display(Model.List)
        </div>
    }
    else {
        @T("There are no subscriptions")
    }

</fieldset>